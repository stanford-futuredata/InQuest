{
    "oracle": {
        "strategy": "oracle",
        "oracle_csv": "datasets/final-precomputed/venice-grand-canal-2018-01-20-true.csv",
        "columns": ["car_count"],
        "frame_col": "false"
    },
    "proxy": {
        "model": "precomputed",
        "proxy_csv": "datasets/final-precomputed/venice-grand-canal-2018-01-20-blazeit-prob.csv",
        "columns": ["proxy"],
        "frame_col": "false"
    },
    "sampling": {
        "strategy": "inquest",
        "num_strata": 3,
        "num_segments": 5,
        "defensive": "true",
        "defensive_frac": 0.1,
        "pilot_sample_frac": 0.2,
        "pilot_query_frac": 0.2,
        "strata_ewm_alpha": 0.8,
        "alloc_ewm_alpha": 0.8,
        "min_strata_gap": 0.001,
        "subsample": 10
    },
    "statistic": {
        "function": "count",
        "count_col": "car_count"
    },
    "aggregation": {
        "function": "mean"
    },
    "query": {
        "type": "aggregation",
        "oracle_limit": [500, 1000, 1500, 2000, 2500, 3000, 3500, 4000, 4500, 5000],
        "start_frame": 0,
        "time_limit": 500000,
        "predicate": "car_count > 0"
    },
    "trials_per_oracle_limit": 1000
}
